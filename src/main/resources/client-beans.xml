<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="carWebService" class="org.springframework.remoting.jaxws.JaxWsPortProxyFactoryBean">
        <property name="serviceInterface" value="com.company.api.CarService"/>
        <property name="wsdlDocumentUrl" value="http://localhost:8080/car?wsdl"/>
        <property name="namespaceUri" value="http://ws.company.com/"/>
        <property name="serviceName" value="CarWSService"/>
        <property name="portName" value="CarWSPort"/>
    </bean>

    <bean id="carClient" class="com.foo.client.CarClientImpl">
        <constructor-arg ref="carWebService"/>
    </bean>



    <bean id="accountWebService" class="org.springframework.remoting.jaxws.JaxWsPortProxyFactoryBean">
        <property name="serviceInterface" value="com.company.api.AccountService"/>
        <property name="wsdlDocumentUrl" value="http://localhost:8080/AccountServiceEndpoint?WSDL"/>
        <property name="namespaceUri" value="http://company.com/"/>
        <property name="serviceName" value="AccountService"/>
        <property name="portName" value="AccountServiceEndpointPort"/>
    </bean>

    <bean id="accountClient" class="com.foo.client.AccountClientImpl">
        <constructor-arg ref="accountWebService"/>
    </bean>
</beans>